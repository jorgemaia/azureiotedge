{
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "sebaderiotedge": {
                "username": "$CONTAINER_REGISTRY_USERNAME_sebaderiotedge",
                "password": "$CONTAINER_REGISTRY_PASSWORD_sebaderiotedge",
                "address": "sebaderiotedge.azurecr.io"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "microsoft/azureiotedge-agent:1.0-preview",
              "createOptions": ""
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "microsoft/azureiotedge-hub:1.0-preview",
              "createOptions": ""
            }
          }
        },
        "modules": {
          "tempSensor": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "microsoft/azureiotedge-simulated-temperature-sensor:1.0-preview",
              "createOptions": ""
            }
          },
          "TaggingModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.TaggingModule.amd64}",
              "createOptions": ""
            }
          },
          "2ndPythonModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.2ndPythonModule}",
              "createOptions": {}
            }
          },
          "SebaderTempGenerator": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.SebaderTempGenerator}",
              "createOptions": {}
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "sensorToTaggingModule": "FROM /messages/modules/tempSensor/outputs/temperatureOutput INTO BrokeredEndpoint(\"/modules/TaggingModule/inputs/input1\")",
          "TaggingModuleToIoTHub": "FROM /messages/modules/TaggingModule/outputs/output1 INTO $upstream",
          "SamplePythonModuleToIoTHub": "FROM /messages/modules/SamplePythonModule/outputs/* INTO $upstream",
          "SampleModuleToIoTHub": "FROM /messages/modules/SampleModule/outputs/* INTO $upstream",
          "2ndPythonModuleToIoTHub": "FROM /messages/modules/2ndPythonModule/outputs/* INTO $upstream",
          "SampleModuleCsharpToIoTHub": "FROM /messages/modules/SampleModuleCsharp/outputs/* INTO $upstream",
          "SebaderTempGeneratorToIoTHub": "FROM /messages/modules/SebaderTempGenerator/outputs/* INTO $upstream",
          "SampleModule2ToIoTHub": "FROM /messages/modules/SampleModule2/outputs/* INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    }
  }
}